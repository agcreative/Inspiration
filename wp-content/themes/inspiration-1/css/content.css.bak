/* Page content | body.page div#content */
body.page div#content {
	height:750px;
	left:20px;
	overflow-x: hidden;
	overflow-y: auto;
	position:absolute;
	top:30px;
	width:780px;
	z-index:5;
	padding-right:10px;
}

body.page div#content h1 {
	color:#009d57;
	font-size:26px;
	font-weight:bold;
	margin:0 0 20px 0;
	text-transform:uppercase;
}

body.page div#content h2,
body.page div#content h3,
body.page div#content h4,
body.page div#content h5,
body.page div#content h6 {
	color:#009d57;
	font-size:14px;
	font-weight:bold;
}

body.page div#content p {
	color:#535454;
	font-size:13px;
	margin:0 0 15px 0;
}

body.page div#content span.green {
	color:#009d57;
	font-weight:bold;
	font-style:italic;
}

body.page div#content ul,
body.page div#content ol {
	margin:0 20px 15px 20px;
}

body.page div#content ul li {
	color:#535454;
	font-size:13px;
	list-style-type:disc;
	margin:0 0 5px 0;
}

body.page div#content ol li {
	color:#535454;
	font-size:13px;
	list-style-type:decimal;
	margin:0 0 5px 0;
}

