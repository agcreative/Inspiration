/* Header | div#header */
div#header a#logo {
	background:url(../images/logo.png) 0 0 no-repeat;
	cursor:pointer;
	display:block;
	height:85px;
	position:absolute;
	right:0;
	text-indent:-9999px;
	top:0;
	width:338px;
}

div#header ul {
	left:435px;
	position:absolute;
	top:35px;
}

div#header ul li {
	border-left:1px solid #ffffff;
	display:block;
	float:left;
	height:13px;
	margin:0;
	padding:0 5px;
}

div#header ul li:first-child {
	border-left:0 none !important;
}

div#header ul li a {
	color:#000000;
	font-size:12px;
	color: #ffffff;
	height:13px;
	line-height:13px;
	text-transform:lowercase;
}

/* Masthead | div.masthead */
#content div#masthead {
	height:308px;
	margin:47px 0 0 0;
	overflow:hidden;
	width:810px;
}

#content div#masthead div#set_1 {
	background:url(../polaroids/polaroid-set-1-photo-1.jpg) 0 0 no-repeat;
	float:left;
	height:308px;
	width:183px;
}

#content div#masthead div#set_2 {
	background:url(../polaroids/polaroid-set-2-photo-1.jpg) 0 0 no-repeat;
	float:left;
	height:308px;
	width:145px;
}

#content div#masthead div#set_3 {
	background:url(../polaroids/polaroid-set-3-photo-1.jpg) 0 0 no-repeat;
	float:left;
	height:308px;
	width:167px;
}

#content div#masthead div#set_4 {
	background:url(../polaroids/polaroid-set-4-photo-1.jpg) 0 0 no-repeat;
	float:left;
	height:308px;
	width:154px;
}

#content div#masthead div#set_5 {
	background:url(../polaroids/polaroid-set-5-photo-1.jpg) 0 0 no-repeat;
	float:left;
	height:308px;
	width:161px;
}

#content div#masthead div {
	position:relative;
}

#content div#masthead div img {
	left:0;
	position:absolute;
	top:0;
	z-index:1;
}

/* Splash Boxes | div.box-left, div.box-right */
body.splash #content .box {
	position:relative;
}

#content .box-left {
	border-right:1px solid #abacad;
	float:left;
	height:400px;
	overflow:hidden;
	padding:0 0 0 34px;
	width:393px;
}

#content .box-left img {
	bottom:0;
	left:34px;
	position:absolute;
}

#content .box-right {
	float:left;
	height:400px;
	overflow:hidden;
	padding:0 0 0 10px;
	width:372px;
}

#content .box-right img {
	margin:0 0 45px 0;
}

body.splash div.box h2 {
	color:#231f20;
	font-size:17px;
	font-weight:bold;
	margin:0 0 10px 0;
	text-transform:uppercase;
}

body.splash div.box h2 a {
	font-size:13px;
	font-weight:normal;
	text-transform:lowercase;
}

body.splash div.box p {
	font-size:13px;
}

